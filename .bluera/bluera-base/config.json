{
  "version": 1,
  "autoLearn": {
    "enabled": true,
    "mode": "suggest",
    "threshold": 3,
    "target": "local"
  },
  "milhouse": {
    "defaultMaxIterations": 0,
    "defaultStuckLimit": 3,
    "defaultGates": []
  },
  "notifications": {
    "enabled": true
  },
  "autoCommit": {
    "enabled": true,
    "onStop": true,
    "push": false,
    "remote": "origin"
  },
  "dryCheck": {
    "enabled": true,
    "onStop": true,
    "threshold": 5,
    "minTokens": 70,
    "minLines": 5
  },
  "strictTyping": {
    "enabled": true
  },
  "standardsReview": {
    "enabled": true,
    "mode": "warn"
  },
  "deepLearn": {
    "enabled": true,
    "model": "haiku",
    "maxBudget": 0.02,
    "autoPromoteEnabled": true,
    "autoPromoteThreshold": 0.9
  },
  "memory": {
    "enabled": true,
    "surfaceOnStart": false,
    "contextBudget": 500
  }
}
